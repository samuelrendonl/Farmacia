            <Image url="@../Imagenes/DICLOFENACO%20GEL.png" />
         </image>
      </ImageView>
      <Button fx:id="btnHome" layoutX="50.0" layoutY="24.0" mnemonicParsing="false" onAction="#HomeAction" opacity="0.0" prefHeight="36.0" prefWidth="52.0" style="-fx-cursor: hand;" text="btnHome" />
      <Button fx:id="btnMenu" layoutX="1117.0" layoutY="16.0" mnemonicParsing="false" onAction="#MenuAction" opacity="0.0" prefHeight="54.0" prefWidth="71.0" style="-fx-cursor: hand;" text="btnMenu" />
