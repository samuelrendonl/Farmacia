      <Button fx:id="btnAggCarrito" layoutX="889.0" layoutY="526.0" mnemonicParsing="false" onAction="#AggCarritoAction" opacity="0.0" prefHeight="62.0" prefWidth="284.0" style="-fx-cursor: hand;" text="btnAggCarrito" />
      <Button fx:id="btnComprar" layoutX="889.0" layoutY="599.0" mnemonicParsing="false" onAction="#ComprarAction" opacity="0.0" prefHeight="62.0" prefWidth="284.0" style="-fx-cursor: hand;" text="btnComprar" />
      <ComboBox fx:id="comboBuscar" layoutX="174.0" layoutY="16.0" prefHeight="47.0" prefWidth="621.0" promptText="Buscar..." />
   </children>
</AnchorPane>
